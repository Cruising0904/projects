{
  "name": "remon-browsersdk",
  "version": "0.2.4",
  "description": "RemoteMonster Browser SDK",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --progress",
    "test": "karma start --single-run",
    "test:dev": "karma start",
    "e2e": "nightwatch --env local",
    "lint": "eslint ./src",
    "clean": "npm run clean:dist && npm run clean:doc",
    "clean:dist": "rm -rf ./dist",
    "clean:doc": "rm -rf ./doc",
    "compile-babel": "babel --presets es2015 -d dist/ src/",
    "prepublish": "npm run build",
    "build": "npm run clean && npm run build:production && npm run build:doc && npm run build:changelog",
    "build:dev": "webpack --progress --display-modules --env dev",
    "build:production": "webpack --progress --display-modules --env build",
    "build:doc": "esdoc",
    "build:changelog": "standard-changelog",
    "package:template": "cp -r template/* release",
    "package:sdk": "cp dist/remon.min.js release",
    "package:doc": "cp -r doc release",
    "package:changelog": "cp CHANGELOG.md release",
    "package": "npm run build && npm run package:template && npm run package:sdk && npm run package:changelog",
    "deploy:npm": "서브모듈에서 npm 실행",
    "deploy:gh-pages": "서브모듈에서 npm 실행 doc과 examples가 올라감",
    "deploy:upload:remotemonster.com": "scp -rp dist/* ubuntu@remotemonster.com:/tmp/browser-sdk/dist && scp -rp dev/* ubuntu@remotemonster.com:/tmp/browser-sdk",
    "deploy": ""
  },
  "author": "RemoteMonster",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-tape-runner": "^2.0.1",
    "chromedriver": "^2.27.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.2.0",
    "exports-loader": "^0.6.3",
    "faucet": "^0.0.1",
    "firefox-profile": "^0.4.6",
    "i": "^0.3.5",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-tap": "^3.0.1",
    "karma-tap-pretty-reporter": "^3.0.0",
    "karma-webpack": "^2.0.0",
    "nightwatch": "^0.9.8",
    "standard-changelog": "^1.0.1",
    "tap-diff": "^0.1.1",
    "tap-difflet": "^0.7.0",
    "tap-markdown": "^1.2.1",
    "tap-min": "^1.1.0",
    "tap-notify": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0",
    "webpack": "^2.1.0",
    "webpack-dev-server": "^2.1.0",
    "webpack-uglify-js-plugin": "^1.1.9",
    "yargs": "^7.0.2"
  },
  "dependencies": {
    "inline-worker": "^1.1.0",
    "platform": "^1.3.3",
    "webrtc-adapter": "^2.0.3"
  }
}
